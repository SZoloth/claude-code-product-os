# Copy this file to .env.local and fill in your values

# Database URL (if using a database)
# DATABASE_URL="postgresql://username:password@localhost:5432/database_name"

# NextAuth.js (if using authentication)
# NEXTAUTH_URL="http://localhost:3000"
# NEXTAUTH_SECRET="your-secret-key"

# External APIs
# EXTERNAL_API_URL="https://api.example.com"
# EXTERNAL_API_KEY="your-api-key"

# Analytics (if using analytics)
# GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"
# MIXPANEL_TOKEN="your-mixpanel-token"

# Email service (if using email)
# EMAIL_SERVER_HOST="smtp.gmail.com"
# EMAIL_SERVER_PORT="587"
# EMAIL_SERVER_USER="your-email@example.com"
# EMAIL_SERVER_PASSWORD="your-password"
# EMAIL_FROM="noreply@yourdomain.com"

# File upload (if using file uploads)
# UPLOADTHING_SECRET="sk_live_..."
# UPLOADTHING_APP_ID="your-app-id"

# Payment processing (if using payments)
# STRIPE_PUBLIC_KEY="pk_live_..."
# STRIPE_SECRET_KEY="sk_live_..."
# STRIPE_WEBHOOK_SECRET="whsec_..."

# Feature flags
# FEATURE_FLAG_NEW_UI="true"
# FEATURE_FLAG_BETA_FEATURES="false"